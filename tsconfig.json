{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "target": "es6",
    "module": "es6",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
